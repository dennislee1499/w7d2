<h2>Sign Up</h2>
<input type='hidden' name='authencitity_token' value='<%= form_authenticity_token %>' >
<form action="<%= users_url %>%>" method="POST">
    <label for="email">Email:
        <input type="text" id="email" name="user[email]" value="<%= @user.email %>">
    </label>
    <br/>
    <label for="password">Password:
        <input type="text" id="password" name="user[password]" value="<%= @user.password %>">
    </label>
    <br/>
</form>